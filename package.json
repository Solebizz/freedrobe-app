{
	"name": "app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"dev:inspect": "npm install && node --inspect=0.0.0.0:9175 node_modules/vite/bin/vite.js dev",
		"dev:setup": "./.devcontainer/devSetup.sh",
		"package": "node --loader ts-node/esm ./app-packaging/package.ts",
		"emulator": "node .devcontainer/emulator.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@capacitor/assets": "^3.0.5",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/luxon": "^3.4.2",
		"@types/sortablejs": "^1.15.8",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.11.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"gh-pages": "^6.1.1",
		"jsdom": "^24.1.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.77.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"ts-node": "^10.9.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"@capacitor/splash-screen": "^6.0.0",
		"@capacitor/status-bar": "^6.0.0",
		"@capacitor/android": "^6.1.0",
		"@capacitor/app": "^6.0.0",
		"@capacitor/cli": "^6.1.0",
		"@capacitor/core": "^6.1.0",
		"@capacitor/ios": "^6.1.0",
		"luxon": "^3.4.4",
		"sortablejs": "^1.15.2"
	}
}
